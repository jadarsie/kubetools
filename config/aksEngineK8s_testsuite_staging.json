{
	"clusterARMTemplate": {
		"githubRepo": "msazurestackworkloads/azurestack-gallery",
		"githubBranch": "ci-test",
		"folderPath": "kubernetes/template/DeploymentTemplates",
		"dvmLogFilePath": "/var/log/azure/deploy-script-dvm.log",
		"aksEngine": {
			"githubRepo": "msazurestackworkloads/aks-engine",
			"githubBranch": "next",
			"apiModel":"https://raw.githubusercontent.com/honcao/aks-engine/e2e/examples/kubernetes-azure-stack.json",
			"upgradeVersion": "1.12",
			"nodeCount": 5,
			"upgrade": "true",
			"scale": "true"
		},
		"minNumberofAzsNodes": 1,
		"removeDeployment": "true",
		"deploymentMatrix": [
			{
				"agentPoolProfileCount": 3,
				"agentPoolProfilevmSize": "Standard_D2_v2",
				"masterPoolProfileCount": 1,
				"masterPoolProfilevmSize": "Standard_D2_v2",
				"storageProfile": "BlobDisk",
				"hyperkubeImagesVersion": "1.11"
			},
			{
				"agentPoolProfileCount": 3,
				"agentPoolProfilevmSize": "Standard_D2_v2",
				"masterPoolProfileCount": 3,
				"masterPoolProfilevmSize": "Standard_D2_v2",
				"storageProfile": "ManagedDisk",
				"hyperkubeImagesVersion": "1.11"
			}
		]
	},
	"diagnosis": {
		"githubRepo": "msazurestackworkloads/azurestack-gallery",
		"githubBranch": "master",
		"folderPath": "diagnosis",
		"filename": "getkuberneteslogs.sh"
	},
	"testSuite": {
		"githubRepo": "msazurestackworkloads/kubetools",
		"githubBranch": "master",
		"applications": [
			{
				"name": "sonobuoy",
				"junitFile": "junit_01.xml",
				"deployment": {
					"scriptName": "deploy.sh",
					"summaryFile": "deploy.json"
				},
				"validation": {
					"scriptName": "validate.sh",
					"summaryFile": "validate.json"
				},
				"cleanup": {
					"scriptName": "cleanup.sh",
					"summaryFile": "cleanup.json"
				}
			},
			{
				"name": "wordpress",
				"deployment": {
					"scriptName": "deploy.sh",
					"summaryFile": "deploy.json"
				},
				"validation": {
					"scriptName": "validate.sh",
					"summaryFile": "validate.json"
				},
				"cleanup": {
					"scriptName": "cleanup.sh",
					"summaryFile": "cleanup.json"
				}
			}
		]
	}
